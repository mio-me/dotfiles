[user]
	name = Thorben Below
	email = below.thorben@inside-online.de
[core]
    editor = nvim
    pager = delta

[interactive]
    diffFilter = delta --color-only

[pull]
	rebase = true
[rebase]
	autoStash = true
[commit]
	gpgsign = true
[gpg]
	program = gpg
[tag]
	gpgsign = true
[init]
	defaultBranch = main
[color]
	ui = auto
